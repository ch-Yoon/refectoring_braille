<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/menu_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/AppBasicColor"
    android:gravity="center"
    android:orientation="vertical"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:nextFocusDown="@+id/menu_layout"
    android:nextFocusLeft="@+id/menu_layout"
    android:nextFocusUp="@+id/menu_layout"
    android:nextFocusRight="@+id/menu_layout"
    tools:context="com.project.why.braillelearning.Menu.MenuActivity">

    <ImageView
        android:id="@+id/braillelearningmenu_imageview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@color/AppBasicColor" />

</LinearLayout>
